{
  "parser": "babel-eslint",
  "env": {
    "node": true
  },
  "rules": {
    "eol-last": 2,
    "max-len": [2, 80, 4],
    "no-underscore-dangle": [0],
    "no-var": 2,
    "quotes": [2, "single"],
    "space-before-function-paren": [2, {
      "anonymous": "always",
      "named": "never"
    }],
    "strict": [2, "global"],
    "semi": [2, "always"]
  }
}
